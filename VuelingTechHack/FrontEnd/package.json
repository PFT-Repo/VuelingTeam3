{
  "name": "team3",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --config .prettierrc --list-different \"(src|projects)/**/*{.ts,.html,.scss}\"",
    "format:fix": "prettier --config .prettierrc --write \"(src|projects)/**/*{.ts,.html,.scss}\"",
    "stubs": "stubby -s 1500 -l 127.0.0.1 -d stubbs/stubby.json -w",
    "stubby": "concurrently \"npm run stubs\" \"npm start\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.1.5",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.1.5",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "chart.js": "^4.2.1",
    "rxjs": "~7.5.0",
    "sweetalert2": "^11.7.2",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "15.0.0-alpha.5",
    "@angular-eslint/eslint-plugin": "15.0.0-alpha.5",
    "@angular-eslint/eslint-plugin-template": "15.0.0-alpha.5",
    "@angular-eslint/schematics": "15.0.0-alpha.5",
    "@angular-eslint/template-parser": "15.0.0-alpha.5",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
